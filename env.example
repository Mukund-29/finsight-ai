# Docker Compose Environment Variables
# Copy this file to .env and update with your values

# Backend Port (host port mapping)
BACKEND_PORT=8081

# Frontend Port (host port mapping)
FRONTEND_PORT=4200

# Spring Profile (local, h2, or production)
SPRING_PROFILE=local

# Database Configuration
SPRING_DATASOURCE_URL=jdbc:oracle:thin:@your-host:1521:your-database
SPRING_DATASOURCE_USERNAME=your_username
SPRING_DATASOURCE_PASSWORD=your_password
SPRING_DATASOURCE_DRIVER_CLASS_NAME=oracle.jdbc.OracleDriver
SPRING_JPA_PROPERTIES_HIBERNATE_DEFAULT_SCHEMA=your_schema

# Backend API URL (for frontend to connect)
# Use this if frontend and backend are on different hosts
# BACKEND_API_URL=http://your-backend-host:8081
